{
  "name": "clean-node-api",
  "version": "2.5.6",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rm -rf dist && tsc -p tsconfig-build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "build:watch": "rm -rf dist && tsc -p tsconfig-build.json -w",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "dev:local": "sucrase-node src/main/server.ts",
    "check": "npm-check -s -u",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "jest:clearCache": "jest --clearCache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn run test:ci && coveralls < coverage/lcov.info"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/graphql": "^14.5.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "apollo-server-integration-testing": "^3.0.0",
    "apollo-server-types": "^3.5.1",
    "bson-objectid": "^2.0.2",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "eslint": "^7.18.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "mockdate": "^3.0.5",
    "npm-check": "^5.9.2",
    "sucrase": "^3.20.3",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.2",
    "@graphql-tools/utils": "^8.6.2",
    "apollo-server-express": "^3.6.3",
    "bcrypt": "^5.0.1",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^4.4.0",
    "nodemon": "^2.0.15",
    "swagger-ui-express": "^4.1.6",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
